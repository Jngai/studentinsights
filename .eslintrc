{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "_": true,
    "moment": true,
    "$": true,
    "React": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "jasmine": true,
    "expect": true,
    "it": true,
    "d3": true,
    "Highcharts": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["react"],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "jsx-a11y/href-no-hash": 0,
    "func-names": 0,
    "prefer-rest-params": 0,
    "eqeqeq": 0,
    "import/extensions": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "max-len": 0,
    "no-shadow": 0,
    "no-plusplus": 0,
    "camelcase": 0,
    "consistent-return": 0,
    "no-throw-literal": 0,
    "no-nested-ternary": 0,
    "react/jsx-no-bind": 0,

    "no-restricted-properties": [2, {
      "object": "Object",
      "property": "assign",
      "message": "Not supported by IE 11, try _.clone."
    }]
  }
}
